-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Webcontainer Security Collaborator
Bundle-Description: Webcontainer Security Collaborator Common Classes, version=${bVersion}
Bundle-SymbolicName: com.ibm.ws.webcontainer.security

  
Export-Package: \
  com.ibm.websphere.security.web, \
  com.ibm.ws.webcontainer.security, \
  com.ibm.ws.webcontainer.security.context, \
  com.ibm.ws.webcontainer.security.metadata, \
  com.ibm.ws.webcontainer.security.util

Import-Package: com.ibm.ws.common.internal.encoder, !*.internal.*, !com.ibm.ws.kernel.boot.cmdline, *

Private-Package: \
  com.ibm.ws.webcontainer.security.internal.*, \
  com.ibm.ws.webcontainer.security.jacc, \
  com.ibm.ws.webcontainer.security.resources

DynamicImport-Package: \
  com.ibm.wsspi.request.probe.*

Service-Component: \
  com.ibm.ws.webcontainer.security.configurator;\
    implementation:=com.ibm.ws.webcontainer.security.metadata.SecurityServletConfiguratorHelperFactory;\
    provide:=com.ibm.ws.container.service.config.ServletConfiguratorHelperFactory;\
    configuration-policy:=ignore;\
    immediate:=true;\
    properties:="service.vendor=IBM",\
  com.ibm.ws.webcontainer.security.LoggedOutTokenCacheImpl;\
    implementation:=com.ibm.ws.webcontainer.security.LoggedOutTokenCacheImpl;\
    configuration-policy:=ignore;\
    activate:=activate;\
    deactivate:=deactivate;\
    tokenManager=com.ibm.ws.security.token.TokenManager;\
    dynamic:='tokenService';\
    properties:="service.vendor=IBM"

instrument.classesExcludes: com/ibm/ws/webcontainer/security/resources/*.class

-buildpath: \
	com.ibm.websphere.appserver.spi.kernel.service,\
	com.ibm.websphere.appserver.spi.logging,\
	com.ibm.websphere.org.osgi.core.6.0.0,\
	com.ibm.websphere.org.osgi.service.component.1.3.0,\
	com.ibm.websphere.javaee.servlet.3.0;version=latest,\
	com.ibm.ws.security;version=latest,\
	com.ibm.ws.security.authentication;version=latest,\
	com.ibm.ws.kernel.security.thread,\
	com.ibm.ws.security.authorization;version=latest,\
	com.ibm.ws.security.registry;version=latest,\
	com.ibm.ws.common.encoder;version=latest, \
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.adaptable.module;version=latest,\
	com.ibm.ws.webcontainer;version=latest,\
	com.ibm.ws.security.credentials;version=latest,\
	com.ibm.websphere.security;version=latest,\
	com.ibm.ws.security.authentication.tai;version=latest,\
	com.ibm.websphere.javaee.annotation.1.1;version=latest,\
	com.ibm.ws.javaee.dd;version=latest,\
	com.ibm.ws.javaee.dd.common;version=latest,\
	com.ibm.ws.anno;version=latest,\
	com.ibm.ws.dynacache;version=latest,\
	com.ibm.ws.security.jaas.common;version=latest,\
	com.ibm.ws.security.token;version=latest,\
	com.ibm.ws.transport.http;version=latest,\
	com.ibm.ws.logging;version=latest,\
	com.ibm.ws.security.audit.context;version=latest, \
	com.ibm.ws.security.audit.utils;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning.1.0.0;version=latest

-testpath: \
	../build.sharedResources/lib/junit/old/junit.jar;version=file, \
	../build.sharedResources/lib/hamcrest-all.jar;version=file, \
	../build.sharedResources/lib/jmock/old/jmock-junit4.jar;version=file, \
	../build.sharedResources/lib/jmock/old/jmock.jar;version=file, \
	../build.sharedResources/lib/jmock/legacy/jmock-legacy.jar;version=file, \
	../build.sharedResources/lib/jmock/legacy/objenesis.jar;version=file, \
	../build.sharedResources/lib/jmock/legacy/cglib-nodep.jar;version=file, \
	../build.sharedResources/lib/ws-junit/ws-junit.jar;version=file, \
	../build.sharedResources/lib/httpclient/4.1.2/httpclient-4.1.2.jar;version=file, \
	../build.sharedResources/lib/httpclient/4.1.2/httpcore-4.1.2.jar;version=file, \
	../build.sharedResources/lib/httpclient/4.1.2/commons-logging-1.1.1.jar;version=file, \
	../build.sharedResources/lib/httpclient/4.1.2/commons-codec-1.4.jar;version=file, \
	com.ibm.ws.kernel.boot;version=latest, \
	com.ibm.websphere.org.osgi.service.cm.1.5.0, \
	com.ibm.ws.injection;version=latest, \
	com.ibm.ws.org.apache.commons.io.1.4;version=latest, \
	com.ibm.ws.org.apache.commons.fileupload.1.2.1;version=latest
