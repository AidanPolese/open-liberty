-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Third Party JPA Container
Bundle-SymbolicName: com.ibm.ws.jpa.container.thirdparty
Bundle-Description: Container-managed Third Party JPA support, version ${bVersion}

WS-TraceGroup: JPA

-dsannotations: com.ibm.ws.jpa.container.thirdparty.ThirdPartyJPAProvider

Export-Package: com.ibm.ws.jpa.container.thirdparty

-buildpath: \
	com.ibm.websphere.appserver.spi.logging;version=latest,\
	com.ibm.websphere.javaee.persistence.2.1;version=latest,\
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.websphere.org.osgi.service.component;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.jpa.container.core;version=latest,\
	com.ibm.ws.org.osgi.annotation.versioning;version=latest
