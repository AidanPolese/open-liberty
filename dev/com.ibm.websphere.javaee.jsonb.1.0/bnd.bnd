-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: com.ibm.websphere.javaee.jsonb.1.0

Require-Capability: osgi.ee; filter:="(&(osgi.ee=JavaSE)(version=1.8))"

Export-Package: \
   javax.json.bind, \
   javax.json.bind.adapter, \
   javax.json.bind.annotation, \
   javax.json.bind.config, \
   javax.json.bind.serializer, \
   javax.json.bind.spi

Include-Resource: \
  @${repo;javax.json.bind-api;0.2.99.b01}!/META-INF/NOTICE

instrument.disabled: true

publish.wlp.jar.suffix: dev/api/spec

-buildpath: \
	${javac.bootclasspath.java6}, \
	javax.json.bind-api;version=0.2.99.b01
