# For information on bnd syntax, see http://was.pok.ibm.com/xwiki/bin/view/Liberty/UsingBnd
#
#Imported but not exported:
#com.ibm.ejs.util.am

-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: dynacache
Bundle-SymbolicName: com.ibm.ws.dynacache
Bundle-Description: Dynacache Core cache capability in Liberty

IBM-Default-Config: OSGI-INF/wlp/defaultInstances.xml

Include-Resource: \
    OSGI-INF=resources/OSGI-INF, \
    OSGI-INF/l10n=resources/OSGI-INF/l10n
    
Service-Component: \
 com.ibm.ws.cache.deserialization; \
    implementation:=com.ibm.ws.serialization.DeserializationClassProvider;\
    provide:=com.ibm.ws.serialization.DeserializationClassProvider;\
    properties:="packages=com.ibm.ws.cache;service.vendor=IBM",\
 com.ibm.ws.cache.util.SerializationUtility;\
    implementation:=com.ibm.ws.cache.util.SerializationUtility;\
    serializationService=com.ibm.ws.serialization.SerializationService;\
    properties:="service.vendor=IBM"

Export-Package: \
 com.ibm.websphere.cache*; provide:=true, \
 com.ibm.websphere.exception, \
 com.ibm.ws.cache.resources;version="1.0.16", \
 com.ibm.ws.cache*; provide:=true, \
 com.ibm.wsspi.cache; provide:=true

Import-Package: com.ibm.ws.common.internal.encoder, !*.internal.*, !com.ibm.ws.kernel.boot.cmdline, *

-dsannotations: \
  com.ibm.ws.cache.MBeans, \
  com.ibm.ws.cache.Scheduler, \
  com.ibm.ws.cache.ServerCache, \
  com.ibm.ws.cache.CacheServiceImpl, \
  com.ibm.ws.cache.ObjectCacheServiceImpl, \
  com.ibm.ws.cache.DefaultCacheProvider

instrument.disabled: true

-buildpath: \
	com.ibm.websphere.appserver.spi.logging,\
	com.ibm.websphere.appserver.spi.kernel.service,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.classloading;version=latest,\
	com.ibm.websphere.org.osgi.core.6.0.0,\
	com.ibm.websphere.org.osgi.service.cm.1.5.0,\
	com.ibm.websphere.org.osgi.service.component.1.3.0,\
	com.ibm.wsspi.org.osgi.service.component.annotations.1.3.0,\
	com.ibm.websphere.javaee.servlet.3.0;version=latest,\
	com.ibm.ws.common.encoder;version=latest,\
	com.ibm.ws.crypto.passwordutil,\
	com.ibm.ws.serialization;version=latest,\
	com.ibm.ws.resource;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning.1.0.0;version=latest

# <property name="extra.vmargs" value="-Xmx1g" />

-testpath: \
	../build.sharedResources/lib/junit/old/junit.jar;version=file, \
	../build.sharedResources/lib/ws-junit/ws-junit.jar;version=file, \
	../build.sharedResources/lib/hamcrest-all.jar;version=file, \
	../build.sharedResources/lib/jmock/old/jmock-junit4.jar;version=file, \
	../build.sharedResources/lib/jmock/old/jmock.jar;version=file
