-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

# For more information, see http://was.pok.ibm.com/xwiki/bin/view/Liberty/UsingBnd

Bundle-Name: Security Microprofile JWT proxy
Bundle-SymbolicName: com.ibm.ws.security.mp.jwt.proxy
Bundle-Description: Security Microprofile JWT proxy service, version=${bVersion}
Bundle-ActivationPolicy: lazy

WS-TraceGroup: \
  Security
																																																															
Export-Package: \
  com.ibm.ws.security.mp.jwt.proxy; provide:=true

Import-Package: !*.internal.*, *
   
-dsannotations: \
  com.ibm.ws.security.mp.jwt.proxy.MpJwtHelper
  
-buildpath: \
	com.ibm.ws.logging.core;version=latest, \
	com.ibm.ws.kernel.service, \
    com.ibm.ws.kernel.boot, \
	com.ibm.websphere.org.osgi.core, \
	com.ibm.websphere.org.osgi.service.component, \
	com.ibm.wsspi.org.osgi.service.component.annotations, \
	com.ibm.ws.org.osgi.annotation.versioning;version=latest
