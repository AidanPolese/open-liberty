# For information on bnd syntax, see http://was.pok.ibm.com/xwiki/bin/view/Liberty/UsingBnd

-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: JDBC Management for J2EE JSR77
Bundle-SymbolicName: com.ibm.ws.jdbc.management.j2ee
Bundle-Description: JDBC Management for J2EE JSR77; version=${bVersion}

#optional, should match the trace group used by Tr components (see sample package-info.java)
WS-TraceGroup: RRA

# For each exported package, create (in that package) a package-info.java
# file, and place an @version javadoc tag in package-level javadoc. 
# Append ";provide:=true" if this bundle also provides an implementation
# for the exported API.

Private-Package: com.ibm.ws.jdbc.management.j2ee.*

-dsannotations: com.ibm.ws.jdbc.management.j2ee.internal.JDBCMBeanRuntime

-buildpath: \
	com.ibm.websphere.appserver.api.j2eemanagement;version=latest, \
	com.ibm.websphere.appserver.spi.kernel.service;version=latest,\
	com.ibm.websphere.appserver.spi.logging;version=latest,\
	com.ibm.websphere.org.osgi.core.6.0.0;version=latest,\
	com.ibm.websphere.org.osgi.service.component.1.3.0;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations.1.3.0;version=latest,\
	com.ibm.ws.classloading;version=latest,\
	com.ibm.ws.resource;version=latest
