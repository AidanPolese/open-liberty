<project name="com.ibm.ws.connectionpool-api" default="buildApiSpi">
  <!-- DO NOT CHANGE THE DEFAULT TASK FROM 'buildApiSpi'
       Top level builds use this task explicity.  Changing the
       default will create an incompatibility between a default
       project build and a top level build. -->
       
  <!-- IBM-API required properties. -->
  <property name="bundleName" value="WebSphere ConnectionPool Stats" />
  <property name="api.ibm.symbolic.suffix" value="connectionpool" />
  <property name="api.ibm.version" value="1.0"/>  
  <property name="feature.name" value="com.ibm.websphere.appserver.connectionpoolmonitor-1.0"/>
        
  <import file="../ant_build/public_imports/apibnd_imports.xml"/>
  <property name="additional.javadoc.src.dirs" value="${basedir}/../com.ibm.websphere.connectionpool.monitor/src/"/>
 
</project>