-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: WAS Security JASPI
Bundle-Description: WAS Security JASPI 1.1, version=${bVersion}
Bundle-SymbolicName: com.ibm.ws.security.jaspic.1.1

Require-Capability: osgi.ee; filter:="(&(osgi.ee=JavaSE)(version=1.7))"

WS-TraceGroup: \
  JASPI

Export-Package: \
  com.ibm.wsspi.security.jaspi, \
  com.ibm.ws.security.jaspi;thread-context=true, \
  com.ibm.ws.security.jaspi.client

Import-Package: com.ibm.ws.common.internal.encoder, !*.internal.*, !com.ibm.ws.kernel.boot.cmdline, *
 
Private-Package: \
  com.ibm.ws.security.jaspi.internal.resources

-dsannotations: \
com.ibm.ws.security.jaspi.JaspiServiceImpl, \
com.ibm.ws.security.jaspi.JaspiServletContainerInitializer

-dsannotations-inherit: true

instrument.classesExcludes: com/ibm/ws/security/jaspi/internal/resources/*.class

-buildpath: \
	com.ibm.websphere.appserver.spi.logging,\
	com.ibm.websphere.javaee.jaspic.1.1;version=latest,\
	com.ibm.websphere.security;version=latest,\
	com.ibm.websphere.javaee.servlet.3.0;version=latest,\
	com.ibm.websphere.org.osgi.core.6.0.0,\
	com.ibm.websphere.org.osgi.service.component.1.3.0,\
	com.ibm.wsspi.org.osgi.service.component.annotations.1.3.0,\
	com.ibm.ws.security;version=latest,\
	com.ibm.ws.security.authentication;version=latest,\
	com.ibm.ws.webcontainer.security;version=latest,\
	com.ibm.websphere.appserver.spi.kernel.service,\
	com.ibm.ws.security.token;version=latest,\
	com.ibm.ws.webcontainer;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning.1.0.0;version=latest
