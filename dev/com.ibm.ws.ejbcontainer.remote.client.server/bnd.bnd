-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

# For more information, see http://was.pok.ibm.com/xwiki/bin/view/Liberty/UsingBnd

Bundle-Name: EJB enablement feature for Client Container Remote Support
Bundle-SymbolicName: com.ibm.ws.ejbcontainer.remote.client.server
Bundle-Description: Client EJB, version ${bVersion}

#optional, should match the trace group used by Tr components (see sample package-info.java)
WS-TraceGroup: EJBContainer

# For each exported package, create (in that package) a package-info.java
# file, and place an @version javadoc tag in package-level javadoc. 
# Append ";provide:=true" if this bundle also provides an implementation
# for the exported API.

Private-Package: com.ibm.ws.ejbcontainer.remote.client.server.internal

Import-Package:\
 javax.rmi.*;version="[2.4,3)",\
 org.omg.*;version="[2.4,3)",\
 *
 
DynamicImport-Package: *
 
-dsannotations-inherit: true

-dsannotations: \
  com.ibm.ws.ejbcontainer.remote.client.server.internal.ClientEJBFactoryImpl

-buildpath: \
	com.ibm.websphere.appserver.spi.logging;version=latest,\
	com.ibm.websphere.javaee.ejb.3.1;version=latest, \
	com.ibm.websphere.org.osgi.core.6.0.0;version=latest,\
	com.ibm.websphere.org.osgi.service.component.1.3.0;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations.1.3.0;version=latest,\
	com.ibm.ws.clientcontainer.remote.common;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.ejbcontainer;version=latest
