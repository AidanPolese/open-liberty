-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: com.ibm.ws.security.jsr375.cdi
Bundle-Description: WAS Security JSR-375 Security 1.0 CDI, version=${bVersion}
Bundle-SymbolicName: com.ibm.ws.security.jsr375.cdi

Require-Capability: osgi.ee; filter:="(&(osgi.ee=JavaSE)(version=1.8))"

WS-TraceGroup: \
    JSR375

Export-Package: \
    com.ibm.ws.security.jsr375.cdi

Import-Package: !*.internal.*, !com.ibm.ws.kernel.boot.cmdline, *
 
Private-Package: \
    com.ibm.ws.security.jsr375.cdi.internal.resources

-dsannotations: \
    com.ibm.ws.security.jsr375.cdi.JSR375CDIExtension

-dsannotations-inherit: true

instrument.classesExcludes: com/ibm/ws/security/jsr375/internal/resources/*.class

Include-Resource: \
    META-INF=resources/META-INF

-buildpath: \
    com.ibm.websphere.javaee.annotation.1.2;version=latest,\
    com.ibm.websphere.javaee.interceptor.1.2;version=latest,\
    com.ibm.websphere.javaee.jaspic.1.1;version=latest,\
    com.ibm.websphere.javaee.jsr375.1.0;version=latest,\
    com.ibm.websphere.security;version=latest,\
    com.ibm.websphere.javaee.servlet.3.0;version=latest,\
    com.ibm.websphere.org.osgi.core,\
    com.ibm.websphere.org.osgi.service.component,\
    com.ibm.wsspi.org.osgi.service.component.annotations,\
    com.ibm.ws.cdi.1.2.interfaces, \
    com.ibm.ws.logging;version=latest,\
    com.ibm.ws.security;version=latest,\
    com.ibm.ws.security.authentication;version=latest,\
    com.ibm.ws.security.jaspic.1.1;version=latest,\
    com.ibm.ws.security.jsr375.1.0;version=latest,\
    com.ibm.ws.webcontainer.security;version=latest,\
    com.ibm.websphere.appserver.spi.kernel.service,\
    com.ibm.ws.security.token;version=latest,\
    com.ibm.ws.webcontainer;version=latest, \
    com.ibm.ws.org.osgi.annotation.versioning;version=latest, \
    com.ibm.websphere.javaee.cdi.1.2;version=latest

-testpath: \
    ../build.sharedResources/lib/junit/old/junit.jar;version=file, \
    ../build.sharedResources/lib/ws-junit/ws-junit.jar;version=file, \
    org.hamcrest:hamcrest-all;version=1.3, \
    org.jmock:jmock-junit4;version=2.5.1, \
    org.jmock:jmock;version=2.5.1, \
    ../build.sharedResources/lib/jmock/legacy/jmock-legacy.jar;version=file, \
    org.objenesis:objenesis;version=1.0, \
    ../build.sharedResources/lib/jmock/legacy/cglib-nodep.jar;version=file, \
    com.ibm.ws.cdi.1.2.interfaces, \
    com.ibm.websphere.javaee.annotation.1.2;version=latest,\
    com.ibm.websphere.javaee.cdi.1.2;version=latest,\
    com.ibm.websphere.javaee.el.3.0;version=latest,\
    com.ibm.ws.security.jsr375.1.0;version=latest,\
    com.ibm.ws.kernel.boot;version=latest, \
    com.ibm.ws.container.service;version=latest, \
    com.ibm.ws.security.registry;version=latest

