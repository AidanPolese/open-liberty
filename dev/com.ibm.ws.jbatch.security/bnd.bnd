# For information on bnd syntax, see http://was.pok.ibm.com/xwiki/bin/view/Liberty/UsingBnd

-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Batch Security
Bundle-SymbolicName: com.ibm.ws.jbatch.security
Bundle-Description: Batch Role Based Security Support; version=${bVersion}

WS-TraceGroup: wsbatch

Private-Package:\
  com.ibm.ws.jbatch.security.*

-dsannotations:\
  com.ibm.ws.jbatch.security.impl.WSBatchAuthServiceImpl

-buildpath: \
	com.ibm.websphere.appserver.spi.kernel.service;version=latest,\
	com.ibm.websphere.appserver.spi.logging;version=latest,\
	com.ibm.websphere.javaee.batch.1.0;version=latest,\
	com.ibm.websphere.org.osgi.core.6.0.0;version=latest,\
	com.ibm.websphere.org.osgi.service.component.1.3.0;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations.1.3.0;version=latest,\
	com.ibm.jbatch.container;version=latest,\
	com.ibm.jbatch.spi;version=latest,\
	com.ibm.ws.logging.core;version=latest,\
	com.ibm.ws.security;version=latest,\
	com.ibm.ws.security.authorization;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning.1.0.0;version=latest
