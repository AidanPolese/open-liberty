-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

javac.source: 1.7
javac.target: 1.7

Bundle-Name: JAX-RS BeanValidation
Bundle-SymbolicName: com.ibm.ws.jaxrs.2.0.beanvalidation
Bundle-Description: IBM JAX-RS-2.0 BeanValidation support; version=${bVersion}

WS-TraceGroup: JAXRS

# jaxrs-2.0-beanValidation is part of EE7 and therefore requires java 1.7 
Require-Capability: osgi.ee;filter:="(&(osgi.ee=JavaSE)(version=1.7))" 

Import-Package: \
  org.apache.cxf.*;version="[3.0.2,4)", \
  com.ibm.websphere.*, \
  javax.ws.rs.*, \
  javax.validation;version="[1.0,2)", \
  javax.validation.executable.*;resolution:=optional;version="[1.0,2)", \
  javax.validation.*;version="[1.0,2)", \
  com.ibm.ws.jaxrs20.api, \
  com.ibm.ws.jaxrs20.api.*, \
  com.ibm.ws.ffdc, \
  com.ibm.ws.ffdc.*, \
  com.ibm.ws.beanvalidation.service, \
  com.ibm.ejs.util, \
  com.ibm.ws.runtime.metadata, \
  com.ibm.ws.threadContext, \
  com.ibm.wsspi.kernel.service.utils, \
  org.osgi.framework, \
  org.osgi.service.component

  
Export-Package: \
  org.apache.cxf.validation.*;version=3.1.8, \
  org.apache.cxf.jaxrs.validation.*;version=3.1.8, \
   
# If you need use MESSAGE, you must enable this Private-Package, or message will translate wrong
Private-Package:\
   com.ibm.ws.jaxrs20.beanvalidation.*

-dsannotations: com.ibm.ws.jaxrs20.beanvalidation.component.*    
 
-buildpath: \
	../com.ibm.ws.org.apache.cxf.jaxrs/lib/cxf-core-3.1.8.jar;version=file,\
	../com.ibm.ws.org.apache.cxf.jaxrs/lib/cxf-rt-frontend-jaxrs-3.1.8.jar;version=file,\
	com.ibm.websphere.org.osgi.core.6.0.0;version=latest,\
	com.ibm.websphere.org.osgi.service.component.1.3.0;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations.1.3.0;version=latest,\
	com.ibm.ws.jaxrs.2.0.common;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.kernel.service;version=latest,\
	com.ibm.ws.logging.core;version=latest,\
	com.ibm.websphere.javaee.validation.1.1;version=latest,\
	com.ibm.websphere.javaee.validation.1.0;version=latest,\
	com.ibm.ws.beanvalidation.core;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations.1.3.0
