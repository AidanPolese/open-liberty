<FindBugsFilter>
  <!-- Begin Global Findbugs Excludes do not edit by hand - edit globally-->
  <Match>
    <Bug pattern="BC_IMPOSSIBLE_INSTANCEOF,BC_VACUOUS_INSTANCEOF,BSHIFT_WRONG_ADD_PRIORITY,CO_COMPARETO_INCORRECT_FLOATING,DE_MIGHT_IGNORE,DLS_DEAD_LOCAL_STORE,DM_BOXED_PRIMITIVE_FOR_PARSING,DM_BOXED_PRIMITIVE_TOSTRING,DM_NUMBER_CTOR,DMI_HARDCODED_ABSOLUTE_FILENAME,EQ_DOESNT_OVERRIDE_EQUALS,INT_BAD_COMPARISON_WITH_NONNEGATIVE_VALUE,IT_NO_SUCH_ELEMENT,ME_ENUM_FIELD_SETTER,ME_MUTABLE_ENUM_FIELD,MS_CANNOT_BE_FINAL,MS_MUTABLE_ARRAY,MS_MUTABLE_COLLECTION,MS_MUTABLE_COLLECTION_PKGPROTECT,MS_PKGPROTECT,NP_LOAD_OF_KNOWN_NULL_VALUE,NP_NULL_ON_SOME_PATH,NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE,NP_NULL_PARAM_DEREF,OBL_UNSATISFIED_OBLIGATION_EXCEPTION_EDGE,RANGE_ARRAY_INDEX,RCN_REDUNDANT_NULLCHECK_OF_NULL_VALUE,RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE,RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE,RpC_REPEATED_CONDITIONAL_TEST,RV_RETURN_VALUE_IGNORED_NO_SIDE_EFFECT,SA_FIELD_SELF_ASSIGNMENT,SQL_PREPARED_STATEMENT_GENERATED_FROM_NONCONSTANT_STRING,STCAL_STATIC_SIMPLE_DATE_FORMAT_INSTANCE,UC_USELESS_CONDITION,UC_USELESS_CONDITION_TYPE,UC_USELESS_OBJECT,UC_USELESS_VOID_METHOD,WMI_WRONG_MAP_ITERATOR"/>
  </Match>
  <!-- End Global Findbugs Excludes do not edit by hand - edit globally-->
  <!-- See http://findbugs.sourceforge.net/manual/filter.html for details of the syntax of this file -->

  <!-- ignore all from aries & asm -->
  <Match>
  	<Package name="~org\..*"/>
  </Match>
  
  <!-- match generated nls classes: return mutable type -->
  <Match> 
    <Class name="~.*\.resources\..*" />
    <Method name="getContents"/>
    <Bug code="EI" />
  </Match>
  
  <!-- The featuresetgenerator is not finished yet..  -->
  <Match>
    <Class name="com.ibm.ws.featureverifier.internal.FeatureSetGenerator"/>
  </Match>
  
  <!-- we only use currentMillis for timestamping the xml -->
  <Match>
     <Class name="com.ibm.ws.featureverifier.internal.FeatureVerifier"/>
     <Bug pattern="SYSTEM_CURRENT_TIME_MILLIS_CALLED"/>
  </Match>
  
  <!-- null check is inside a debug check. -->
  <Match>
     <Class name="com.ibm.ws.featureverifier.internal.FeatureVerifier"/>
     <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE"/>
  </Match>
  
  <!-- we're not expecting to need to lock down the codepage, if we do we'll need to fix these. -->
  <Match>
    <Class name="com.ibm.ws.featureverifier.internal.FeatureVerifier"/>
    <Bug pattern="DM_DEFAULT_ENCODING"/>    
  </Match>
  <Match>
    <Class name="com.ibm.ws.featureverifier.internal.XmlErrorCollator"/>
    <Bug pattern="DM_DEFAULT_ENCODING"/>    
  </Match>
  <Match>
    <Class name="com.ibm.aries.buildtasks.semantic.versioning.model.XMLBaseLineUtils"/>
    <Bug pattern="DM_DEFAULT_ENCODING"/>    
  </Match>
  <Match>
    <Class name="com.ibm.aries.buildtasks.semantic.versioning.model.decls.FieldDeclaration"/>
    <Bug pattern="DM_DEFAULT_ENCODING"/>    
  </Match>
  
  <!-- yes, they expose their internals a little, but it doesn't hurt..  much. -->
  <Match>
  	<Class name="com.ibm.aries.buildtasks.semantic.versioning.model.decls.ClassDeclaration"/>
  	<Bug pattern="EI_EXPOSE_REP"/>
  </Match>
  <Match>
  	<Class name="com.ibm.aries.buildtasks.semantic.versioning.model.decls.ClassDeclaration"/>
  	<Bug pattern="EI_EXPOSE_REP2"/>
  </Match>
  <Match>
  	<Class name="com.ibm.aries.buildtasks.semantic.versioning.model.decls.MethodDeclaration"/>
  	<Bug pattern="EI_EXPOSE_REP2"/>
  </Match>
  <Match>
  	<Class name="~com.ibm.ws.featureverifier.internal.FilterUtils.*"/>
  	<Bug pattern="EI_EXPOSE_REP"/>
  </Match>
  
  <!--  Not actually a serial version uid.. just a field representing the uid from the class the decl is representing. -->
  <Match>
  	<Class name="com.ibm.aries.buildtasks.semantic.versioning.model.decls.ClassDeclaration"/>
  	<Bug pattern="SE_NONSTATIC_SERIALVERSIONID"/>
  </Match>
  
  <!-- Existing use of unbox/rebox from the original api checker code..   -->
  <Match>
  	<Class name="com.ibm.aries.buildtasks.semantic.versioning.model.decls.LiveClassDeclaration"/>
  	<Bug pattern="BX_UNBOXING_IMMEDIATELY_REBOXED"/>
  </Match>
  
  <!-- Sys err and sys out are used to form the log when verifying. This is NOT a shipped feature. -->
  <Match>
    <Bug pattern="SYSTEM_ERR_USED"/>
  </Match>
  <Match>
    <Bug pattern="SYSTEM_OUT_USED"/>
  </Match>
  <Match>
    <Bug pattern="THROWABLE_PRINT_STACK_TRACE_CALLED"/>
  </Match>
 
  <!-- A lot of file handling etc, but no do privs. This is NOT a shipped feature, it will not be run with java 2 security. -->
  <Match>
    <Bug pattern="DJV_CONSTRUCT_INSIDE_DO_PRIVILEGED"/>
  </Match>
  <Match>
    <Bug pattern="DJV_DO_INSIDE_DO_PRIVILEGED"/>
  </Match>
  
  <!-- This isn't production code, so a few strings are concat'd without a stringbuffer -->
  <Match>
    <Bug pattern="SBSC_USE_STRINGBUFFER_CONCATENATION"/>
  </Match>
  
  <!-- Not the end of the world.. quite a few caused by integrating older code to new uses -->
  <Match>
    <Bug pattern="DLS_DEAD_LOCAL_STORE"/>
  </Match>
  
  <!-- its sort of 2 maps being iterated, not too worried about this one. -->
  <Match>
  	<Class name="com.ibm.ws.featureverifier.internal.FeatureInfoUtils"/>
  	<Bug pattern="WMI_WRONG_MAP_ITERATOR"/>
  </Match>
  
  <!-- Temporary list of kernel features.. needs a new home. -->
  <Match>
  	<Class name="com.ibm.ws.featureverifier.internal.FeatureVerifier"/>
  	<Bug pattern="MS_PKGPROTECT"/>
  </Match>
  
  <!-- Inherited code from the original API Checker -->
  <Match>
    <Class name="com.ibm.aries.buildtasks.utils.RequiredBundle"/>
    <Bug pattern="EQ_CHECK_FOR_OPERAND_NOT_COMPATIBLE_WITH_THIS"/>
  </Match>
  <Match>
    <Class name="com.ibm.aries.buildtasks.utils.RequiredBundle"/>
    <Bug pattern="HE_EQUALS_USE_HASHCODE"/>
  </Match>
  <Match>
  	<Class name="com.ibm.aries.buildtasks.utils.BundleManifest"/>
  	<Bug pattern="OS_OPEN_STREAM"/>
  </Match>
  <Match>
  	<Class name="com.ibm.aries.buildtasks.utils.BundleManifest"/>
  	<Bug pattern="SE_NONSTATIC_SERIALVERSIONID"/>
  </Match>
  <Match>
  	<Class name="com.ibm.aries.buildtasks.utils.BundleManifest"/>
  	<Bug pattern="IM_BAD_CHECK_FOR_ODD"/>
  </Match>
  
</FindBugsFilter>
