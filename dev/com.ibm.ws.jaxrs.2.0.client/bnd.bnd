-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

javac.source: 1.7
javac.target: 1.7

Bundle-SymbolicName: com.ibm.ws.jaxrs.2.0.client
Bundle-Name: JavaEE Client API for JAX-RS 2.0
Bundle-Description: JavaEE Client API for JAX-RS 2.0, version ${bVersion}

WS-TraceGroup: JAXRS

# jaxrs-2.0 is part of EE7 and therefore requires java 1.7
Require-Capability: osgi.ee;filter:="(&(osgi.ee=JavaSE)(version=1.7))" 


Export-Package: \
  com.ibm.ws.jaxrs20.client;thread-context=true
  
Import-Package: \
   org.apache.cxf.*;version=3.1.8, \
   com.ibm.websphere.security.saml2;resolution:=optional, \
   *
   
Private-Package:\
  com.ibm.ws.jaxrs20.client.*

Service-Component: \
  com.ibm.ws.jaxrs20.client.support.BundleRuntimeProviderFactoryServiceGateway; \
     configuration-policy:=optional; \
     providerFactoryService/setProviderFactoryService=com.ibm.ws.jaxrs20.api.JaxRsProviderFactoryService;\
     immediate:=true; \
     activate:=activate; \
     deactivate:=deactivate; \
     properties:="service.vendor=IBM"
 
 -dsannotations: com.ibm.ws.jaxrs20.client.component.*

instrument.classesExcludes: com/ibm/ws/jaxrs20/client/internal/resources/*.class
    
-buildpath: \
	${javac.bootclasspath.java7}, \
	../com.ibm.ws.org.apache.cxf.jaxrs/lib/cxf-core-3.1.8.jar;version=file,\
	../com.ibm.ws.org.apache.cxf.jaxrs/lib/cxf-rt-frontend-jaxrs-3.1.8.jar;version=file,\
	../com.ibm.ws.org.apache.cxf.jaxrs/lib/cxf-rt-rs-client-3.1.8.jar;version=file,\
	../com.ibm.ws.org.apache.cxf.jaxrs/lib/cxf-rt-transports-http-3.1.8.jar;version=file,\
	com.ibm.ws.jaxrs.2.0.common;version=latest,\
	com.ibm.ws.adaptable.module;version=latest,\
	com.ibm.ws.artifact;version=latest,\
	com.ibm.ws.artifact.overlay;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.webcontainer.security;version=latest,\
	com.ibm.websphere.javaee.jaxrs.2.0;version=latest,\
	com.ibm.websphere.javaee.servlet.3.0;version=latest,\
	com.ibm.websphere.security;version=latest,\
	com.ibm.websphere.appserver.spi.logging,\
	com.ibm.websphere.org.osgi.core.6.0.0,\
	com.ibm.websphere.org.osgi.service.component.1.3.0,\
	com.ibm.websphere.appserver.spi.kernel.service,\
	com.ibm.wsspi.org.osgi.service.component.annotations.1.3.0,\
	com.ibm.websphere.appserver.api.saml20;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning.1.0.0;version=latest
