-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Messaging Msgstore
Bundle-SymbolicName: com.ibm.ws.messaging.msgstore
Bundle-Description: Messaging Msgstore ; version=${bVersion}

  
Private-Package: \
 com.ibm.ws.sib.msgstore.persistence.objectManager,\
 com.ibm.ws.objectManager,\
 com.ibm.ws.objectManager.utils,\
 com.ibm.ws.objectManager.utils.concurrent.atomic,\
 com.ibm.ws.objectManager.utils.concurrent.locks,\
 com.ibm.ws.sib.msgstore.cache.links,\
 com.ibm.ws.sib.msgstore.cache.ref,\
 com.ibm.ws.sib.msgstore.cache.statemodel,\
 com.ibm.ws.sib.msgstore.expiry,\
 com.ibm.ws.sib.msgstore.deliverydelay,\
 com.ibm.ws.sib.msgstore.list,\
 com.ibm.ws.sib.msgstore.impl,\
 com.ibm.ws.sib.msgstore.persistence,\
 com.ibm.ws.sib.msgstore.persistence.dispatcher,\
 com.ibm.ws.sib.msgstore.persistence.impl,\
 com.ibm.ws.sib.msgstore.persistence.objectManager,\
 com.ibm.ws.sib.msgstore.task,\
 com.ibm.ws.sib.msgstore.transactions.impl,\
 com.ibm.ws.sib.msgstore.util

Service-Component: \
 com.ibm.ws.messaging.msgstore;\
   implementation:=com.ibm.ws.sib.msgstore.impl.MessageStoreImpl;\
   provide:=com.ibm.ws.sib.msgstore.MessageStore;\
   alarmManager=com.ibm.ejs.util.am.AlarmManager; \
   itemInterface=com.ibm.ws.sib.processor.ItemInterface;\
   configuration-policy:=ignore;\
   properties:="service.vendor=IBM"

instrument.disabled: true

-buildpath: \
	com.ibm.websphere.appserver.spi.logging;version=latest,\
	com.ibm.websphere.appserver.spi.kernel.service;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.logging.core;version=latest,\
	com.ibm.ws.messaging.common;version=latest,\
	com.ibm.ws.messaging.utils;version=latest,\
	com.ibm.websphere.org.osgi.core.6.0.0;version=latest,\
	com.ibm.websphere.org.osgi.service.component.1.3.0;version=latest,\
	com.ibm.websphere.javaee.transaction.1.1;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning.1.0.0;version=latest

#  <property name="haltOnFailure" value="no"/>

-testpath: \
	../build.sharedResources/lib/junit/old/junit.jar;version=file, \
	../build.sharedResources/lib/ws-junit/ws-junit.jar;version=file, \
	../build.sharedResources/lib/hamcrest-all.jar;version=file, \
	../build.sharedResources/lib/jmock/old/jmock-junit4.jar;version=file, \
	../build.sharedResources/lib/jmock/old/jmock.jar;version=file, \
	com.ibm.ws.messaging_test.fw;version=latest
