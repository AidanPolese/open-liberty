-include= ~../cnf/resources/bnd/bundle.props
bVersion=${bundle.version}

Bundle-Name: Apache CXF JMX Management
Bundle-Description: Apache CXF JMX Management, version ${original.jar.version}
Bundle-SymbolicName: ${symbolic.name}

Export-Package: \
  org.apache.cxf.*;version=2.6.2

Import-Package: \
 javax.xml.bind.*;version="[2.2,3)", \
 !org.springframework.*;resolution:=optional;version="[2.5,4)", \
 !org.apache.aries.*;version="[0.3,2)", \
 !org.osgi.service.blueprint.*, \
 org.apache.cxf.*;version="[2.6.2,2.6.3)", \
 *
 
Include-Resource:\
 @build/tmp/lib/cxf-rt-management-2.6.2.jar!/META-INF/cxf/**, \
 @build/tmp/lib/cxf-rt-management-2.6.2.jar!/schemas/configuration/**, \
 @build/tmp/lib/cxf-rt-management-2.6.2.jar!/META-INF/DEPENDENCIES, \
 @build/tmp/lib/cxf-rt-management-2.6.2.jar!/META-INF/LICENSE, \
 @build/tmp/lib/cxf-rt-management-2.6.2.jar!/META-INF/NOTICE, \
 build/tmp/bnd/cxf-rt-management-2.6.2
 