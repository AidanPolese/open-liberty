-include= ~../cnf/resources/bnd/bundle.props
bVersion=${bundle.version}

Bundle-Name: Apache CXF Runtime Simple Frontend
Bundle-Description: Apache CXF Runtime Simple Frontend, version ${original.jar.version}
Bundle-SymbolicName: ${symbolic.name}

Export-Package: \
  org.apache.cxf.*;version=2.6.2

Import-Package: \
 com.sun.tools.xjc.reader.internalizer;resolution:=optional, \
 org.apache.commons.lang.*;resolution:=optional;version="[2.6.2,2.6.3)", \
 !org.springframework.*;resolution:=optional;version="[2.5,4)", \
 !org.apache.aries.*;version="[0.3,2)", \
 !org.osgi.service.blueprint.*, \
 javax.xml.bind.*;version="[2.2,3)", \
 com.ibm.websphere.ras, \
 com.ibm.websphere.ras.annotation, \
 com.ibm.ws.ffdc, \
 org.apache.cxf.*;version="[2.6.2,2.6.3)", \
 *
 
Include-Resource:\
 @build/tmp/lib/cxf-rt-databinding-jaxb-2.6.2.jar!/META-INF/cxf/**, \
 @build/tmp/lib/cxf-rt-databinding-jaxb-2.6.2.jar!/META-INF/DEPENDENCIES, \
 @build/tmp/lib/cxf-rt-databinding-jaxb-2.6.2.jar!/META-INF/LICENSE, \
 @build/tmp/lib/cxf-rt-databinding-jaxb-2.6.2.jar!/META-INF/NOTICE, \
 build/tmp/bnd/cxf-rt-databinding-jaxb-2.6.2
 