-include= ~../cnf/resources/bnd/bundle.props
bVersion=${bundle.version}

Bundle-Name: Apache CXF Runtime WS Addressing
Bundle-Description: Apache CXF Runtime WS Addressing, version ${original.jar.version}
Bundle-SymbolicName: ${symbolic.name}

Export-Package: \
  org.apache.cxf.*;version=2.6.2

Import-Package: \
 javax.xml.bind.*;version="[2.2,3)", \
 !org.springframework.*;resolution:=optional;version="[2.5,4)", \
 !org.apache.aries.*;version="[0.3,2)", \
 !org.osgi.service.blueprint.*, \
 org.apache.cxf.*;version="[2.6.2,2.6.3)", \
 *
 
Include-Resource:\
 @build/tmp/lib/cxf-rt-ws-policy-2.6.2.jar!/META-INF/cxf/**, \
 @build/tmp/lib/cxf-rt-ws-policy-2.6.2.jar!/META-INF/spring.handlers, \
 @build/tmp/lib/cxf-rt-ws-policy-2.6.2.jar!/META-INF/spring.schemas, \
 @build/tmp/lib/cxf-rt-ws-policy-2.6.2.jar!/schemas/**, \
 @build/tmp/lib/cxf-rt-ws-policy-2.6.2.jar!/META-INF/DEPENDENCIES, \
 @build/tmp/lib/cxf-rt-ws-policy-2.6.2.jar!/META-INF/LICENSE, \
 @build/tmp/lib/cxf-rt-ws-policy-2.6.2.jar!/META-INF/NOTICE, \
 build/tmp/bnd/cxf-rt-ws-policy-2.6.2
 
 
# Remove the resources (blueprint/metadata configuration files) in the OSGI-INF directory
# As we have no plan to support that, will add these things back with NLS support in the future 
# @lib/cxf-rt-ws-policy-2.6.2.jar!/OSGI-INF/blueprint/**, \
 