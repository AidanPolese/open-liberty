# Import the original manifest, after we've declared a different Import-Packages
# statement that removes the servlet spec version

-include=~MANIFEST.MF

Import-Package: \
 javax.servlet;version="!",\
 javax.servlet.http;version="!",\
 junit.framework;version="[3.8,4)",\
 org.osgi.framework;version="[1.3,2)",\
 org.osgi.service.http;version="[1.2,2)"
 
Include-Resource: @org.osgi.test.cases.http-4.2.0.jar, \
 tb1.jar=com.ibm.tb1.jar