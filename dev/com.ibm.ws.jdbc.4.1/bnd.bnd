# For information on bnd syntax, see http://was.pok.ibm.com/xwiki/bin/view/Liberty/UsingBnd

javac.source: 1.7
javac.target: 1.7

-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Database Connectivity
Bundle-SymbolicName: com.ibm.ws.jdbc.4.1
Bundle-Description: Database connectivity infrastructure, version ${bVersion}

WS-TraceGroup: RRA

Require-Capability: osgi.ee; filter:="(&(osgi.ee=JavaSE)(version=1.7))"

Export-Package: \
    com.ibm.ws.jdbc.osgi.v41, \
    com.ibm.ws.rsadapter.jdbc.v41

# Register a marker service to enable 4.1 incompatibilities.
Service-Component: \
  com.ibm.ws.jdbc.v41; \
    implementation:=com.ibm.ws.jdbc.osgi.v41.JDBC41Runtime; \
    provide:=com.ibm.ws.jdbc.osgi.JDBCRuntimeVersion; \
    properties:="version=4.1,service.ranking:Integer=41"

instrument.disabled: true

-buildpath.bootclasspath: ${javac.bootclasspath.1.7}

-buildpath: \
	com.ibm.websphere.javaee.connector.1.7;version=latest,\
	com.ibm.websphere.javaee.transaction.1.2;version=latest,\
	com.ibm.websphere.org.osgi.core.6.0.0;version=latest,\
	com.ibm.ws.jca.cm;version=latest,\
	com.ibm.ws.jdbc;version=latest,\
	com.ibm.ws.jdbc.4.0;version=latest,\
	com.ibm.websphere.appserver.spi.logging;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning.1.0.0;version=latest
