-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: com.ibm.websphere.javaee.validation.1.0; singleton:=true

Export-Package: \
	javax.validation,\
	javax.validation.bootstrap,\
	javax.validation.constraints,\
	javax.validation.groups,\
	javax.validation.metadata,\
	javax.validation.spi

Import-Package: !org.apache.geronimo.osgi.registry.api, \
  !org.osgi.framework, \
  !org.osgi.util.tracker

Private-Package: org.apache.geronimo.osgi.locator

Include-Resource: \
  @lib/geronimo-validation_1.0_spec-1.1.jar!/META-INF/NOTICE

instrument.disabled: true

publish.wlp.jar.suffix: dev/api/spec

-buildpath: \
	${javac.bootclasspath.java6}, \
	lib/geronimo-validation_1.0_spec-1.1.jar;version=file
