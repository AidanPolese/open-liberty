<?xml version="1.0" encoding="UTF-8"?>
<project name="com.ibm.ws.kernel.filemonitor_test" default="unittest">
  <property name="javac.boot.classpath" value="1.7"/>

  <target name="checkJavaVersion">
    <condition property="usingJava7">
      <contains string="${java.version}" substring="1.7"/>
    </condition>
  </target>
	
  <import file="../ant_build/public_imports/standard_imports.xml"/>
  <import file="../ant_build/public_imports/unittest_imports.xml"/>

</project>