-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Security Authorization
Bundle-Description: Security Authorization, version ${bVersion}
Bundle-SymbolicName: com.ibm.ws.security.authorization
Bundle-ActivationPolicy: lazy

WS-TraceGroup: \
  Authorization

Export-Package: \
  com.ibm.ws.security.authorization, \
  com.ibm.ws.security.authorization.jacc

-buildpath: \
	com.ibm.websphere.org.osgi.service.cm.1.5.0;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning.1.0.0;version=latest
