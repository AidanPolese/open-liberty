-buildpath: \
	lib/fcg.jar;version=file, \
	lib/xlxpJAXB.jar;version=file, \
	lib/xlxpScanner.jar;version=file, \
	lib/xlxpScannerUtils.jar;version=file, \
	lib/xlxpVM.jar;version=file, \
	lib/xlxpWASParsers.jar;version=file
