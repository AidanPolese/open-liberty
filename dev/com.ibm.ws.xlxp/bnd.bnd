-include= ~../cnf/resources/bnd/bundle.props, \
          ~../cnf/resources/bnd/app-resources.bnd
bVersion=1.0
bSymbolicName=com.ibm.ws.xlxp.1.5.3

Bundle-SymbolicName: com.ibm.ws.xlxp.1.5.3; singleton:=true

# The SPI definition under META-INF/services
app-resources= \
  META-INF/services/javax.xml.stream.XMLOutputFactory | \
  META-INF/services/javax.xml.stream.XMLInputFactory | \
  META-INF/services/javax.xml.stream.XMLEventFactory | \
  META-INF/services/com.ibm.xml.xlxp.jaxb.fallback.context

# Headers for geronimo osgi provider registry to track
Export-Provider: com.ibm.xml.xlxp2.jaxb.JAXBContextFactory
SPI-Provider: true

Import-Package: javax.xml.bind.*; version="[2.2,3)", \
  com.ibm.xml.jaxb.internal.marshaller;resolution:=optional, \
  com.sun.xml.bind.marshaller;resolution:=optional, \
  com.sun.xml.bind.v2, \
  com.ibm.wsspi.classloading, \
  !org.apache.xerces*, \
  !com.ibm*, \
  !org.apache.xml*, \
  *

Export-Package: com.ibm.xml.xlxp2.api.was;version="1.0.16"; packageType="was_internal"; mandatory:=packageType, \
  com.ibm.xml.xlxp2.api.jaxb;version="2.1.0", \
  com.ibm.xml.xlxp2.api.stax;version="2.1.0";thread-context=true, \
  com.ibm.xml.xlxp2.api.util;version="1.1.0", \
  com.ibm.xml.xlxp2.api.wssec;version="2.1.0";thread-context=true, \
  com.ibm.xml.xlxp2.datatype;version="1.1.0", \
  com.ibm.xml.xlxp2.jaxb;version="1.2.0";thread-context=true, \
  com.ibm.xml.xlxp2.jaxb.marshal.impl;version="2.0.0", \
  com.ibm.xml.xlxp2.jaxb.model;version="1.2.0", \
  com.ibm.xml.xlxp2.jaxb.unmarshal.impl;version="2.1.0", \
  com.ibm.xml.xlxp2.runtime;version="2.1.0", \
  com.ibm.xml.xlxp2.scan.util;version="2.1.0"

Private-Package: com.ibm.fcg.*, \
  com.ibm.xml.xlxp2.*, \
  com.ibm.xml.jaxp.*

Include-Resource: \
  @lib/xlxpJAXB.jar!/META-INF/services/*, \
  @lib/xlxpScannerUtils.jar!/META-INF/services/*, \
  META-INF/services=resources/META-INF/services

