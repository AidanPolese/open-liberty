 DECLARE  BBOA1REG                                                              
    ENTRY(                                                                      
          CHAR8,          /* Daemon group name                   INPUT*/        
          CHAR8,          /* Node name                           INPUT*/        
          CHAR8,          /* Server name                         INPUT*/        
          CHAR12,         /* Register name                       INPUT*/        
          INT4,           /* Minimum number of connections       INPUT*/        
          INT4,           /* Maximum number of connections       INPUT*/        
          1 REGFLAGS_PARM,/* Register flags (tx, sec)            INPUT*/        
          RETCD,          /* Return code                        OUTPUT*/        
          RSNCD           /* Reason code                        OUTPUT*/        
         )                                                                      
       OPTIONS( ASSEMBLER LINKAGE(SYSTEM) );                                    
                                                                                
