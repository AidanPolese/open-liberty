 %/* 2-byte signed integer Type */;                                             
                                                                                
 %DCL INT2 CHAR;                                                                
 %INT2 = 'REAL FIXED BINARY(15,0)';                                             
 %ACT INT2;                                                                     
                                                                                
 %/* 4-byte signed integer Type */;                                             
                                                                                
 %DCL INT4 CHAR;                                                                
 %INT4 = 'REAL FIXED BINARY(31,0)';                                             
 %ACT INT4;                                                                     
                                                                                
 %/* 4-byte signed integer Type for Return Code */;                             
                                                                                
 %DCL RETCD CHAR;                                                               
 %RETCD = 'REAL FIXED BINARY(31,0)';                                            
 %ACT RETCD;                                                                    
                                                                                
 %/* 4-byte signed integer Type for Reason Code */;                             
                                                                                
 %DCL RSNCD CHAR;                                                               
 %RSNCD = 'REAL FIXED BINARY(31,0)';                                            
 %ACT RSNCD;                                                                    
                                                                                
 %/* 4-byte signed integer Type for Return Value */;                            
                                                                                
 %DCL RETVL CHAR;                                                               
 %RETVL = 'REAL FIXED BINARY(31,0)';                                            
 %ACT RETVL;                                                                    
                                                                                
 %/* 4-byte single-precision real floating-point number Type */;                
                                                                                
 %DCL FLOAT4 CHAR;                                                              
 %FLOAT4 = 'REAL FLOAT DECIMAL(6)';                                             
 %ACT FLOAT4;                                                                   
                                                                                
 %/* 8-byte double-precision real floating-point number Type */;                
                                                                                
 %DCL FLOAT8 CHAR;                                                              
 %FLOAT8 = 'REAL FLOAT DECIMAL(16)';                                            
 %ACT FLOAT8;                                                                   
                                                                                
 %/* 16-byte extended-precision real floating-point number Type */;             
                                                                                
 %DCL FLOAT16 CHAR;                                                             
 %FLOAT16 = 'REAL FLOAT DECIMAL(18)';                                           
 %ACT FLOAT16;                                                                  
                                                                                
 %/* Variable-length string (used for input parameters) Type */;                
                                                                                
 %DCL VSTRING CHAR;                                                             
 %VSTRING = 'CHAR(255) VAR';                                                    
 %ACT VSTRING;                                                                  
                                                                                
 %/* Variable-length String Parameter Descriptor */;                            
 %DCL VSTRING_PARM CHAR;                                                        
 %VSTRING_PARM = 'CHAR(*) VAR';                                                 
 %ACT VSTRING_PARM;                                                             
                                                                                
 %/* Fixed-length strings */;                                                   
                                                                                
 %DCL CHAR8 CHAR;                                                               
 %CHAR8 = 'CHAR(8)';                                                            
 %ACT CHAR8;                                                                    
                                                                                
 %DCL CHAR12 CHAR;                                                              
 %CHAR12 = 'CHAR(12)';                                                          
 %ACT CHAR12;                                                                   
                                                                                
 %DCL CHAR256 CHAR;                                                             
 %CHAR256 = 'CHAR(256)';                                                        
 %ACT CHAR256;                                                                  
                                                                                
 %/* Register Flags Type (for use by BBOA1REG)                       */;        
 %/*    Note:  when used in combination with PL/I attributes,        */;        
 %/*           (for example, storage attributes such as BASED)       */;        
 %/*           this data type must be coded last.                    */;        
 %/*    Note:  this data type requires that the identifier for       */;        
 %/*           which it is coded also have a level number.           */;        
                                                                                
 %DCL REGFLAGS CHAR;                                                            
 %REGFLAGS = ',                                                                 
       255 trcmod      BIT(1)  UNALIGNED, /* Modify the OLA trace   */          
                                          /*      level @F003691-01A*/          
       255 trcmore     BIT(1)  UNALIGNED, /* Trace lv 2 @F003691-01A*/          
       255 trcsome     BIT(1)  UNALIGNED, /* Trace lv 1 @F003691-01A*/          
       255 *           BIT(26) UNALIGNED, /* Reserved   @F003691-01C*/          
       255 C2Wprop     BIT(1)  UNALIGNED, /* Propagate task security*/          
       255 Trans       BIT(1)  UNALIGNED, /* Transactional ON/OFF   */          
       255 W2Cprop     BIT(1)  UNALIGNED; /* Assert WAS sec in CICS */';        
 %ACT REGFLAGS;                                                                 
 %DCL REG_FLAGS CHAR;                                                           
 %REG_FLAGS = 'REGFLAGS';                                                       
 %ACT REG_FLAGS;                                                                
                                                                                
 %/* Register Flags Parameter Descriptor                             */;        
 %/*    Note:  when used in combination with PL/I attributes,        */;        
 %/*           (for example, storage attributes such as BASED)       */;        
 %/*           this data type must be coded last.                    */;        
 %/*    Note:  this data type requires that the identifier for       */;        
 %/*           which it is coded also have a level number.           */;        
                                                                                
 %DCL REGFLAGS_PARM CHAR;                                                       
 %REGFLAGS_PARM = ',                                                            
       255 /*trcmod*/  BIT(1)  UNALIGNED, /* Modify the OLA trace   */          
                                          /*      level @F003691-01A*/          
       255 /*trcmore*/ BIT(1)  UNALIGNED, /* Trace lv 2 @F003691-01A*/          
       255 /*trcsome*/ BIT(1)  UNALIGNED, /* Trace lv 1 @F003691-01A*/          
       255             BIT(26) UNALIGNED, /* Reserved   @F003691-01C*/          
       255 /*C2Wprop*/ BIT(1)  UNALIGNED, /* Propagate task security*/          
       255 /*Trans*/   BIT(1)  UNALIGNED, /* Transactional ON/OFF   */          
       255 /*W2Cprop*/ BIT(1)  UNALIGNED  /* Assert WAS sec in CICS */';        
 %ACT REGFLAGS_PARM;                                                            
 %DCL REG_FLAGS_PARM CHAR;                                                      
 %REG_FLAGS_PARM = 'REGFLAGS_PARM';                                             
 %ACT REG_FLAGS_PARM;                                                           
                                                                                
 %/* Unregister Flags Type (for use by BBOA1URG)                     */;        
 %/*    Note:  when used in combination with PL/I attributes,        */;        
 %/*           (for example, storage attributes such as BASED)       */;        
 %/*           this data type must be coded last.                    */;        
 %/*    Note:  this data type requires that the identifier for       */;        
 %/*           which it is coded also have a level number.           */;        
                                                                                
 %DCL URGFLAGS CHAR;                                                            
 %URGFLAGS = ',                                                                 
       255 *           BIT(31) UNALIGNED, /* Reserved               */          
       255 Force       BIT(1)  UNALIGNED; /* Force urg request      */';        
 %ACT URGFLAGS;                                                                 
 %DCL URG_FLAGS CHAR;                                                           
 %URG_FLAGS = 'URGFLAGS';                                                       
 %ACT URG_FLAGS;                                                                
                                                                                
 %/* Unregister Flags Parameter Descriptor                           */;        
 %/*    Note:  when used in combination with PL/I attributes,        */;        
 %/*           (for example, storage attributes such as BASED)       */;        
 %/*           this data type must be coded last.                    */;        
 %/*    Note:  this data type requires that the identifier for       */;        
 %/*           which it is coded also have a level number.           */;        
                                                                                
 %DCL URGFLAGS_PARM CHAR;                                                       
 %URGFLAGS_PARM = ',                                                            
       255             BIT(31) UNALIGNED, /* Reserved               */          
       255 /*Force*/   BIT(1)  UNALIGNED  /* Force urg request      */';        
 %ACT URGFLAGS_PARM;                                                            
 %DCL URG_FLAGS_PARM CHAR;                                                      
 %URG_FLAGS_PARM = 'URGFLAGS_PARM';                                             
 %ACT URG_FLAGS_PARM;                                                           
                                                                                
                                                                                
