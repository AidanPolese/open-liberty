<project name="com.ibm.ws.jdbc" default="build">
  <path id="findbugs.extra.class.path">
    <fileset dir="${basedir}/lib">
      <include name="**/*.jar"/>
    </fileset>
  </path>
  <property name="findbugs.use.extra.class.path" value="true"/>
  
  <import file="../ant_build/public_imports/standard_imports.xml"/>
  <property name="instrument.ffdc" value="false" />
  <property name="no.auto.format" value="true"/>
  <property name="no.trace.instrumentation" value="true" />
</project>