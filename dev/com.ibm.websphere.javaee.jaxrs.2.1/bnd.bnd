-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: com.ibm.websphere.javaee.jaxrs.2.1; singleton:=true

Export-Package: \
	javax.ws.rs,\
	javax.ws.rs.client,\
	javax.ws.rs.container,\
	javax.ws.rs.core,\
	javax.ws.rs.ext,\
	javax.ws.rs.sse

Import-Package: \
 com.ibm.ws.jaxrs20.client; resolution:=optional, \
 *

Service-Component: \
  com.ibm.ws.jaxrs20.client.ResourceProvider; \
    implementation:=com.ibm.wsspi.classloading.ResourceProvider; \
    provide:=com.ibm.wsspi.classloading.ResourceProvider; \
    configuration-policy:=optional; \
    properties:="resources=META-INF/services/javax.ws.rs.client.ClientBuilder"

Include-Resource:\
 META-INF/services/javax.ws.rs.client.ClientBuilder=resources/META-INF/services/javax.ws.rs.client.ClientBuilder

instrument.disabled: true

publish.wlp.jar.suffix: dev/api/spec

-buildpath: \
	com.ibm.ws.javax.j2ee:javax.ws.rs-api;version=2.1.09
