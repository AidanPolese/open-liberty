# For information on bnd syntax, see http://was.pok.ibm.com/xwiki/bin/view/Liberty/UsingBnd

-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Repository
Bundle-SymbolicName: com.ibm.ws.repository
Bundle-Description: Project for storing repository client library

WS-TraceGroup: repo.resource

Export-Package: com.ibm.ws.repository.common.enums;version="1.0.16", \
 com.ibm.ws.repository.common.utils.internal;version="1.0.16", \
 com.ibm.ws.repository.connections;version="1.0.16", \
 com.ibm.ws.repository.exceptions;version="1.0.16", \
 com.ibm.ws.repository.resources;version="1.0.16", \
 com.ibm.ws.repository.resources.writeable;version="1.0.16", \
 com.ibm.ws.repository.strategies.writeable;version="1.0.16", \
 com.ibm.ws.repository.transport.client;version="1.0.16", \
 com.ibm.ws.repository.transport.model;version="1.0.16", \
 com.ibm.ws.repository.transport.exceptions;version="1.0.16"
 
Private-Package: com.ibm.ws.repository.resources.internal, \
 com.ibm.ws.repository.connections.internal

-buildpath: \
	${javac.bootclasspath.java6}, \
	lib/client-lib.jar;version=file
